pipeline
{
   agent any
   stages
   {
     stage("please download code")
	 {
	 steps
	 {
	 sh 'echo downnload code'
	 }
	 }
	 stage('test the code')
	 {
	 steps
	 {
	 sh 'echo testing done'
	 }
	 }
	 stage('build the code')
	 {
	 steps
	 {
	 sh 'echo war file created'
	 }
	 }
	 stage('deploy test env')
	 {
	 steps
	 {
	 sh 'echo deploying test env'
	 }
	 }
	 
	
	}
}
